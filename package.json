{
	"name": "sveltekit-template",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint .",
		"format": "prettier --write .",
		"db:start": "docker compose up",
		"db:push": "drizzle-kit push",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:studio": "drizzle-kit studio",
		"migrate:up": "tsx scripts/migrate.ts --up",
		"migrate:down": "tsx scripts/migrate.ts --down",
		"seed:up": "tsx scripts/seed.ts --up",
		"seed:down": "tsx scripts/seed.ts --down",
		"generate:migration": "tsx scripts/generate-migration.ts",
		"start": "node build"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.0",
		"@eslint/js": "^9.39.1",
		"@faker-js/faker": "^10.1.0",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"@sveltejs/adapter-node": "^5.4.0",
		"@sveltejs/kit": "^2.48.5",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/vite": "^4.1.17",
		"@types/node": "^22",
		"@types/winston": "^2.4.4",
		"dotenv": "^17.2.3",
		"drizzle-kit": "^0.31.7",
		"drizzle-orm": "^0.44.7",
		"drizzle-seed": "^0.3.1",
		"eslint": "^9.39.1",
		"eslint-plugin-svelte": "^3.13.0",
		"globals": "^16.5.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "^5.43.8",
		"svelte-check": "^4.3.4",
		"tailwindcss": "^4.1.17",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.47.0",
		"vite": "^7.2.2"
	},
	"dependencies": {
		"@node-rs/argon2": "^2.0.2",
		"postgres": "^3.4.7",
		"winston": "^3.18.3",
		"winston-daily-rotate-file": "^5.0.0",
		"zod": "^4.1.13"
	}
}
